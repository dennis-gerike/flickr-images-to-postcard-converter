  "devDependencies": {
    "@cucumber/cucumber": "10.3.1",
    "@types/jest": "29.5.11",
    "image-size": "1.1.1",
    "jest": "29.7.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
